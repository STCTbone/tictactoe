$(document).ready(function(){
    $("#square_<%= @ai_square %>").after("<td>X</td>");
    $("#square_<%= @ai_square %>").remove();
    $("#square_<%= @player_square %>").after("<td>O</td>");
    $("#square_<%= @player_square %>").remove();
});

